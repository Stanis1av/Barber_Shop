      <tr>
        <th>Имя</th>
        <th>Телефон</th>
        <th>Дата и время</th>
        <th>Парикмахер</th>
        <th>Цвет</th>
      </tr>
    </thead>

    <% @results.each do |row| %>
    <tbody>
      <tr>
        <td><%= row['username'] %></td>
        <td><%= row['phone'] %></td>
        <td><%= row['datestamp'] %></td>
        <td><%= row['hairdresser'] %></td>
        <td><%= row['color'] %></td>
      </tr>
    </tbody>
    <% end %>
  </table>
</div>
